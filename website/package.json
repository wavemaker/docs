{
  "scripts": {
    "examples": "docusaurus-examples",
    "start": "copy-files-from-to && concurrently \"docusaurus-start\" \"npx build-slides --watch\"",
    "start:nw": "concurrently \"docusaurus-start --no-watch\" \"npx build-slides\"",
    "build": "copy-files-from-to && npx build-slides && docusaurus-build",
    "publish-gh-pages": "docusaurus-publish",
    "write-translations": "docusaurus-write-translations",
    "version": "docusaurus-version",
    "rename-version": "docusaurus-rename-version",
    "test:mdlint": "npx markdownlint ../learn",
    "test:brandlint": "textlint  --rule terminology ../learn/**",
    "cypress:local": "cypress open",
    "cypress:prepare-prod": "npm run start:nw & npx wait-on http://localhost:3000/learn && bash changed_md_files.sh",
    "cypress:prod": "cypress run --reporter mochawesome --reporter-options 'reportDir=cypress/report/mochawesome-report,overwrite=false,html=false,json=true,timestamp=mmddyyyy_HHMMss'",
    "cypress:prep-report": "npx mochawesome-merge --reportDir cypress/report/mochawesome-report > cypress/report/mochawesome.json && ps -ef | grep docusaurus-start | grep -v grep | awk '{print $2}' | xargs kill",
    "cypress:e2e": "npm run cypress:prepare-prod && npm run cypress:prod && npm run cypress:prep-report"
  },
  "devDependencies": {
    "@wavemaker.com/wm-remarkjs-theme": "github:wavemaker/wm-remarkjs-theme",
    "aws-sdk": "^2.712.0",
    "concurrently": "5.1.0",
    "copy-files-from-to": "^2.3.0",
    "cypress": "^3.8.3",
    "docusaurus": "^1.14.4",
    "dotenv": "^8.2.0",
    "markdown-to-slides": "1.0.5",
    "markdownlint": "^0.20.3",
    "markdownlint-cli": "^0.19.0",
    "mocha": "^5.2.0",
    "mochawesome": "^4.1.0",
    "mochawesome-merge": "^2.1.0",
    "mochawesome-report-generator": "^4.1.0",
    "optimist": "^0.6.1",
    "remark": "11.0.2",
    "remarkable-admonitions": "^0.2.2",
    "strip-markdown": "^3.1.2",
    "textlint": "^11.6.3",
    "textlint-plugin-html": "^0.2.0",
    "textlint-rule-terminology": "^2.1.4",
    "update": "^0.7.4",
    "wait-on": "^3.3.0"
  }
}
